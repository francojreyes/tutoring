function twoSum(A, n, target):      // Overall: O(nlogn)
    T = new AVL tree                // O(1)
    for i = 0..n-1:                 // O(n)
        if (target - A[i]) in T:    // | O(logn)
            return true             // | O(1)
        else:
            add A[i] to T           // | O(1)
    return false                    // O(1)

    